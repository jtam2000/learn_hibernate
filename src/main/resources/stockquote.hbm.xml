<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 5.3//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-5.3.dtd">

<hibernate-mapping>
    <class name="com.github.jtam2000.stockquotes.StockQuote" table="hbnate_quote">
        <id name="quote_timestamp">
            <generator class="assigned"></generator>
        </id>

        <property name="bid"></property>
        <property name="ticker"></property>
        <property name="ask"></property>
        <property name="currency"></property>
        <property name="available_shares"></property>
        <property name="outstanding_shares"></property>

    </class>

</hibernate-mapping>